.pro {
  background: #eee;
  padding: 8% 5%;
  margin-top: 2px;
  color: #999;
  font-size: 16px;
}

.pro text {
  display: block;
  line-height: 1.5rem;
}

.list {
  background: #f5f5f5;
  margin-top: 1% ;
  border-top:solid 1px #ccc; text-align: center; padding:10px 5px; 
}

.list .title {
  display: block;
  background: #eee;
  color: #333;
}
.list .info {
  display: block;
  background: #fff;
  color: #ccc;
}
